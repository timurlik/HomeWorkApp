public class Main{
	public static void main(String[] args) {
        ifLeapYear(2001);
	}
	public static void ifLeapYear (int x) {
	    if ((x % 4 == 0) && (x % 100 != 0 ) || (x % 400 == 0)) {
      System.out.println("Год високосный");
      }
	    else {System.out.println("Год не високосный");
      }
	}    
}
